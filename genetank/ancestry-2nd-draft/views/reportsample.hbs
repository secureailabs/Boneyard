    <main role="main">
      <div id="empowering-everyone">
        <div class="banner" id="header-banner">
          <div class="color-overlay"></div>
          <figure class="banner-image" id="top-banner-image-figure">
            <img src="images/dna.jpg" class="top-banner-image" />
          </figure>
          <div class="banner-desc">
            <p class="banner-desc-header">Your Genetic Report</p>
            <p class="banner-desc-info">Here's what you can learn from your DNA</p>
          </div>
        </div>
        <div class="content">
          <div class="row">
            <div class="col-md-6">
              <div class="card risks">
                <img class="card-header-icon" src="/images/icons/disease_risks.png"><div class="card-header" style="background-color: #13c2fa">Disease Risks</div>
                <div class="card-highlight">
                  <div>
                    <table class="image-slideshow-container" id="risks-slideshow">
                      <tr>
                        <td class="image-slideshow-nav arrow-left disabled"><</td>
                        <td>
                          <div class="image-slideshow">
                            {{#each data.risks.highlights}}
                            {{#if @index}}
                            <div class="slide" id="risks-{{@index}}" style="display:none">
                            {{else}}
                            <div class="slide" id="risks-{{@index}}">
                            {{/if}}
                              <a href="#">
                                <div class="half text-right"><img src="{{ this.image }}"></div>
                                <div class="half text-left">
                                  <p>{{ this.name }}</p>
                                  <p>{{ this.note }}</p>
                                </div>
                              </a>
                            </div>
                            {{/each}}
                          </div>
                        </td>
                        <td class="image-slideshow-nav arrow-right disabled">></td>
                      </tr>
                    </table>
                  </div>
                </div>
                <div class="card-list">
                  <ul>
                    {{#each data.risks.data}}
                      <a class="card-list-link" href="#"><li class="card-list-item"><div class="pull-left">{{ this.name }}</div><div class="pull-right text-{{ this.color }}">{{ this.note }}</div><div class="clearfix"></div></li></a>
                    {{/each}}
                    <a class="card-list-link" href="/report-sample-risks"><li class="card-list-item card-list-last-item"><div class="text-center">See all disease risks ></div></li></a>
